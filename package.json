{
  "name": "webpack-starter-101",
  "version": "0.1.0-SNAPSHOT",
  "description": "webpack academy tutorial",
  "main": "index.js",
  "repository": "https://github.com/joma74/webpack-starter-101",
  "author": "joma74",
  "license": "MIT",
  "scripts": {
    "list-dependencies-l0": "yarn list --depth 0 > dist/installed-dependencies.l0.txt",
    "list-dependencies-la": "yarn list > dist/installed-dependencies.la.txt",
    "build-debug": "node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js --env.env=prod",
    "start-dev": "webpack-dev-server --env.env=dev --port=9000 --hot",
    "start-prod": "rm -rf dist/* && webpack --hide-modules=true --env.env=prod",
    "stats-prod": "rm -rf dist/* && webpack --hide-modules=true --env.env=prod --profile --env.addons=statswebpack",
    "test-unit-debug": "karma start --autoWatch true src/test/unit/js/karma.unit.config.js",
    "test-unit-once": "karma start --autoWatch false --singleRun true src/test/unit/js/karma.unit.config.js",
    "test-unit-cover": "rm -rf target/coverage && karma start src/test/unit/js/karma.cover.config.js"
  },
  "_moduleAliases": {
    "root@": ".",
    "bu@": "build-utils"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jasmine": "^2.6.0",
    "@types/karma": "^0.13.36",
    "@types/node": "^8.0.26",
    "@types/ramda": "^0.25.4",
    "@types/webpack": "^3.0.10",
    "@types/webpack-env": "^1.13.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.4",
    "karma-script-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "module-alias": "^2.0.1",
    "node-sass": "^4.5.3",
    "prettyjson": "^1.2.1",
    "sass-loader": "^6.0.6",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "curry-template": "^1.0.4",
    "debug": "^3.0.1",
    "normalize.css": "^7.0.0",
    "ramda": "^0.25.0",
    "tachyons": "^4.8.1",
    "ts-enums": "^0.0.6",
    "vue": "^2.5.2"
  }
}
